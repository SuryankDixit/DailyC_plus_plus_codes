#include <iostream>
#include<cmath>

using namespace std;
int main()
{
    int n,flag=0;
    cout<<"Enter number to check whether it is prime or not :"   ;
    cin>>n;
    for(int i=2;i<int(sqrt(n));i++)
    {
        if(n%i==0)
        {
            flag++;
            break;
        }
    }
    if(flag==0)
    cout<<"Prime number.";
    else
    cout<<"Not Prime";
    return 0;
}

#include<iostream>
using namespace std;
int main()
{
    int n,temp;
    cout<<"Enter the size of array :";
    cin>>n;
    int *arr=new int[n];
    cout<<"Enter elements in the array to sort :";
    for(int i=0;i<n;i++)
        cin>>arr[i];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n-1-i;j++)
        {
            if(arr[j]>arr[j+1])
            {
             temp=arr[j];
             arr[j]=arr[j+1];
             arr[j+1]=temp;
            }
        }
        
    }
    cout<<"Sorted array is : \n";
    for(int i=0;i<n;i++)
    cout<<arr[i]<<" ";
}

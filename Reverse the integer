//Reversing the integer.

#include <iostream>
using namespace std;

void reverseInteger(int n)
{
    int sum=0;
    while(n>0)
    {
        sum=sum*10+n%10;
        n=n/10;
    }
    cout<<"Reversed integer is :" <<sum;
}

int main()
{
    cout<<"enter the number :";
    int n;
    cin>>n;
    reverseInteger(n);
    return 0;
}

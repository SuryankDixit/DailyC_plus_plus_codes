#include <stdio.h>
#include <time.h>
#include <stdlib.h>


 int main()
 {
     int n,i;
     printf("Enter array size :");
     scanf("%d",&n);
     int arr[n];
     int key;
     printf("Enter element you want to search :");
     scanf("%d",&key);
     
    srand(time(0)); 
    for(i=0;i<n;i++)
        arr[i]=rand()%100;
     
    clock_t t; 
    t = clock();
    
    int flag=0;
    
    for(i=0;i<n;i++)
    {
        if(arr[i]==key)
        {
        flag=i;
        break;
        }
    }
    
    t = clock() - t; 
    double time_taken = ((double)t)/CLOCKS_PER_SEC;
    printf("Time taken : %f\n", time_taken);
    
    if(flag==0)
    printf("Element not found\n");
    else
    printf("Element is present at %d position\n",flag);
    
 }
